        {
            "file_in": "templates/dlink.html",
            "file_out": "scripts/page_data/admin/dlink.html",
            "mode": "replace"
        },
        
        {
            "file_in": "templates/dframe.html",
            "file_out": "scripts/page_data/admin/dframe.html",
